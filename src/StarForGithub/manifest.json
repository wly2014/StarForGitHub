{
	"name": "StarForGitHub",
	"description": "StarForGitHub",
	"version": "1.0",
	"manifest_version": 2,
	"permissions": [
	"notifications",
	"tabs", 
	"handleResponse",
	"bookmarks", 
	"declarativeWebRequest", 
	"*://*/*"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
		},
		"browser_action": {

			"default_popup": "pop.html",
			"default_icon" : "icon.png",
			"default_title": "Start "
		}
	}